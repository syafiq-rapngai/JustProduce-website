    <!-- JSON-LD markup generated by Google Structured Data Markup Helper. -->
    <script type="application/ld+json">
        {
        "@context" : "http://schema.org",
        "@type" : "LocalBusiness",
        "name" : "Just Produce",
        "image" : "https://justproduce.sg/img/logos/logo-footer.png",
        "telephone" : "+65 89229300",
        "email" : "sales@justproduce.sg",
        "address" : {
            "@type" : "PostalAddress",
            "streetAddress" : "Commonwealth Capital 7 Buroh Lane #06-06",
            "addressCountry" : "Singapore",
            "postalCode" : "618291"
        }
        }
    </script>

    <style type="text/css">
        section:before {
            height: 54px;
            content: "";
            display:block;
        }
    </style>

    <meta name="description" content="Home to your fresh, healthy mesclun salads. Just Produce brings you the freshest, most nutritious and flavourful salads at affordable prices. We grow our vegetables in vertical farms in Singapore, so that we can deliver them to you safe and fresh off the farm." />
    <link rel="stylesheet" type="text/css" href="third_party/slick/slick.css"/>
    <link rel="stylesheet" type="text/css" href="third_party/slick/slick-theme.css"/>
</head>

<body>
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-5238RRK"height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->

    <!-- start page loading -->
    <div id="preloader">
        <div class="row loader">
            <div class="loader-icon"></div>
        </div>
    </div>
    <!-- end page loading -->

    <!-- start main-wrapper section -->
    <div class="main-wrapper">

        <!-- start header section -->
        <header class="header-style1 menu_area-light">
            <%- include ('../partials/header.ejs') %>
        </header>
        <!-- end header section -->

        <!-- start jumbotron-->
        <!-- <div class="container-fluid full-screen header-overlay">
            <div class="box">
                <img class="img-responsive text-center" src="img/index/JustProduce2.jpeg" alt="Salad bowl">
                <div class="text">
                    <h1 class="main-green-header">Welcome to Just Produce</h1>
                    <h2 class="main-black-header">Home to your fresh, healthy mesclun salads</h2>
                </div>
            </div>
        </div> -->

        <div class="container-fluid full-screen" style="min-height: 700px;">
            <div class="row slider-fade ">
                <div class="owl-carousel owl-theme width-100 ">
                    <div class="text-center item bg-img header-overlay" data-overlay-dark="1" data-background="img/index/JustProduce2.jpeg" alt="Salad bowl">
                        <div class="absolute-middle-center caption">
                            <div class="overflow-hidden">
                                <h1 class="header-text padding-20x-bottom text-white header-shadow">Welcome to Just Produce</h1>
                                <br>
                                <p class="margin-30px-bottom md-margin-20px-bottom xs-display-none center-col text-white subheader-shadow"><b>Home to your fresh, healthy mesclun salads</b></p>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <!-- end jumbotron-->

        <!-- start about us section -->
        <section id="about-us" data-aos="fade-up" data-aos-once="true">
            <div class="row content-section padding-20px-bottom">
                <div class=col-8>
                    <h2 class="green-header" >At Just Produce</h2>
                    <h3>We bring you the freshest, most nutritious and flavourful vegetables, just as it is, and just for you.</h3>
                    <button type="button" class="btn btn-success index-page-btn responsive-width" onclick="window.location.href='/about'">Learn more about us</button>
                </div>
                <div class=col-4 style="padding: 0px;">
                    <img src="img/index/Saladbowl1.png">
                </div>
                <br>
            </div>
        </section>
        <!-- end about us section -->

        <!-- start what we offer -->
        <section id="what-we-offer" class="alt-div" data-aos="fade-right" data-aos-once="true">
            <div class="content-section padding-60px-bottom">
                <div class="row col-12">
                    <h2 class="green-header">Safe and healthy vegetables</h2>
                    <h3>No pesticides, preservatives or GMO. Our 100% local selection of salads and vegetables is guaranteed to satisfy your taste buds.</h3>
                </div>
                <div class="row product-carousel" style="margin: auto;">
                    <% for(var i=0; i<products.length; i++) {%>
                        <div class='col-lg-3 col-md-3 col-sm-6 col-xs-6 col-6' style="padding-left: 0px; margin-bottom: 1vh;">
                            <a href="/products#<%= products[i].url %>">
                            <div id="<%= products[i].imageurl %>-frame" class="product-frame" style="background-color: white; border-color: <%= products[i].color %>;;">
                                <div alt="<%= products[i].name %>" data-image-hover="img/products/<%= products[i].imageurl %>/Thumbnail.jpg"/ 
                                style="--bg-image-normal: url('/img/index/<%= products[i].fancy_name %>.jpg');  --bg-image-hover: url('/img/products/<%= products[i].imageurl %>/Opened.jpg');">
                                </div>
                                <h3 id="<%= products[i].imageurl %>-caption" class="product-caption" style="color: <%= products[i].color %>;"><%- (products[i].fancy_name).replace(" ","<br/>"); %></h3>
                            </div>
                            </a>
                        </div>
                    <% } %>       
                </div>
                <div class="row col-12">
                    <button type="button" class="btn btn-success index-page-btn responsive-width" onclick="window.location.href='/products'">Check out our fresh selection</button>
                </div>
            </div>
        </section>
        <!-- end what we offer -->

        <!-- <div class="product-carousel">
            <div><img src="img/content/about.jpg" alt=""></div>
            <div><img src="img/content/about.jpg" alt=""></div>
        </div> -->

        <!-- start where to buy section -->
        <section id="where-to-buy" data-aos="fade-left" data-aos-once="true" class="padding-60px-bottom">
            <div class="row content-section">
                <div class="col-8">
                    <h2 class="green-header">Grown in Singapore</h2>
                    <h3>Ready to get your local salad box? We are available in different online and offline stores across Singapore.</h3>
                    <button type="button" class="btn btn-success index-page-btn responsive-width" onclick="window.location.href='/where-to-buy'">Get your tasty salad boxes</button> 
                </div>
                <div class="col-4">
                    <div class="box-shadow-primary padding-15px-all bg-white" style="border-radius: 10px;">
                        <img src="img/index/GrowninSingapore.png" style="border-radius: 10px;">
                    </div>
                </div>
            </div>

            
        </section>
        <!-- end where to buy -->
        
        <!-- Start Contact Form -->
        <section id="contact-form" data-aos="fade-up" data-aos-once="true" class="alt-div">
            <%- include ('./contact-form-partial.ejs') %>
        </section>
        <!-- End Contact Form -->

        <!-- Start Instagram Embedded Feed -->
        <section data-aos="fade-up" data-aos-once="true" class="padding-20px-bottom">
            <div class="row content-section">
                <script src="https://apps.elfsight.com/p/platform.js" defer></script>
                <div class="elfsight-app-fe4bfbce-ffce-4091-b54e-f3587bc715f2"></div>
            </div>
        </section>
        <!-- End Instagram Embedded Feed -->

            <!-- start footer section -->
        <footer>
            <%- include ('../partials/footer.ejs') %>
        </footer>
        <!-- end footer section -->
        <div id="snackbar">Email Copied!</div>

        <!-- start scroll to top -->
    <a href="javascript:void(0)" class="scroll-to-top top-arrow-center"><i class="fas fa-angle-up" aria-hidden="true"></i></a>
    <!-- end scroll to top -->

    <!-- all js include start -->

    <!-- jQuery -->
    <script src="main_dependencies/js/jquery.min.js"></script>

    <!-- modernizr js -->
    <script src="main_dependencies/js/modernizr.js"></script>

    <!-- bootstrap -->
    <script src="main_dependencies/js/bootstrap.min.js"></script>

    <!-- Serch -->
    <script src="main_dependencies/search/search.js"></script>

    <!-- navigation -->
    <script src="main_dependencies/js/nav-menu.js"></script>

    <!-- tab -->
    <script src="main_dependencies/js/easy.responsive.tabs.js"></script>

    <!-- owl carousel -->
    <script src="main_dependencies/js/owl.carousel.min.js"></script>

    <!-- jquery.counterup.min -->
    <script src="main_dependencies/js/jquery.counterup.min.js"></script>

    <!-- stellar js -->
    <script src="main_dependencies/js/jquery.stellar.min.js"></script>

    <!-- waypoints js -->
    <script src="main_dependencies/js/waypoints.min.js"></script>

    <!-- tab js -->
    <script src="main_dependencies/js/tabs.min.js"></script>

    <!-- jquery.magnific-popup js -->
    <script src="main_dependencies/js/jquery.magnific-popup.min.js"></script>

    <!-- isotope.pkgd.min js -->
    <script src="main_dependencies/js/isotope.pkgd.min.js"></script>

    <!-- map js -->
    <script src="main_dependencies/js/map.js"></script>

    <!-- custom scripts -->
    <script src="js/main.js"></script>

    <!-- contact form scripts -->
    <script src="main_dependencies/js/mailform/jquery.form.min.js"></script>
    <script src="main_dependencies/js/mailform/jquery.rd-mailform.min.c.js"></script>

    <!-- Chatfuel-chatbox script -->
    <%- include ('../partials/chatfuel-chatbox.ejs') %>

    <link href="aos/dist/aos.css" rel="stylesheet"> 
    <script src="aos/dist/aos.js"></script>
    <script>
      AOS.init({
        delay: 100,
        duration:400
    });
    </script>

    <script type="text/javascript" src="js/contact.js"></script>
    <script type="text/javascript" src="js/contact-form.js"></script>

    <script type="text/javascript" src="third_party/slick/slick.min.js"></script>

    <script>
        $(document).ready(full_screen_carousell());
        function full_screen_carousell()
        {
            $('.product-carousel').slick({
                infinite: true,
                slidesToShow: 4,
                slidesToScroll: 1,
                swipe: true,
                autoplay: true,
                autoplaySpeed: 2000,
                pauseOnHover: true,
                responsive: [
                {
                    breakpoint: 800, //at 600px wide, only 2 slides will show
                    settings: {
                        slidesToShow: 2
                    }
                }
                ]
            });
        }
    </script>
    <!-- all js include end -->
      
    <script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit"
        async defer>
    </script>

</body>

</html>